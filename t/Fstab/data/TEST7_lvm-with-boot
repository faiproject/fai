disk_config sda disklabel:msdos bootable:1 align-at:4k
primary /boot 1250-1300  ext4    rw
primary swap  1024       swap    rw
logical -     4096-      -       -

disk_config lvm
vg my_vg sda5
my_vg-root    /          1024    ext4    rw,errors=remount-ro
my_vg-tmp     /tmp       1024    ext4    rw,nosuid
my_vg-varlog  /var/log   1024    ext4    rw
my_vg-home    /home      1024-   ext4    rw
