@fstab = (
           '# /etc/fstab: static file system information.',
           '#',
           '# <file sys>	<mount point>	<type>	<options>	<dump>	<pass>',
           '/dev/sda3	/	xfs	rw	0	1',
           '/dev/sda1	/boot	ext2	rw	0	2',
           '/dev/sda2	none	swap	sw	0	0'
         );
%disk_var = (
              'SWAPLIST' => '"/dev/sda2"',
              'BOOT_DEVICE' => '"/dev/sda"',
              'ROOT_PARTITION' => '/dev/sda3',
              'BOOT_PARTITION' => '/dev/sda1'
            );
