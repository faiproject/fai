@fstab = (
           '# /etc/fstab: static file system information.',
           '#',
           '# <file sys>	<mount point>	<type>	<options>	<dump>	<pass>',
           'UUID=11e86ce7-2589-420d-9833-5186c2cd8a61	/	ext4	rw,noatime,errors=remount-ro	0	1	# device at install: /dev/md0',
           '/dev/sda2	none	swap	sw	0	0',
           '/dev/sdb2	none	swap	sw	0	0',
         );
%disk_var = (
              'SWAPLIST' => '"/dev/sda2 /dev/sdb2"',
              'BOOT_DEVICE' => '"/dev/md0"',
              'ROOT_PARTITION' => 'UUID=11e86ce7-2589-420d-9833-5186c2cd8a61'
            );
